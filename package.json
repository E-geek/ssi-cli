{
  "name": "ssi-cli",
  "version": "0.0.5",
  "description": "build files wih ssi directives by cli",
  "main": "bin/ssi.js",
  "scripts": {
    "test": "mocha --timeout 5000 -s 500 -c -r chai --compilers coffee:coffee-script/register tests ",
    "cover": "istanbul cover -x 'lib/*.js' ./node_modules/.bin/_mocha -- --compilers coffee:coffee-script/register -r coffee-coverage/register-istanbul -r chai tests"
  },
  "bin": {
    "ssi": "./bin/ssi"
  },
  "keywords": [
    "ssi",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/E-geek/ssi-cli.git"
  },
  "author": "E-geek",
  "license": "MIT",
  "dependencies": {
    "coffee-script": "=1.9.3",
    "concat-stream": "=1.5.1",
    "coveralls": "^2.11.9",
    "cson": "=3.0.2",
    "node-ssi": "=0.3.1",
    "optimist": "=0.6.1"
  },
  "devDependencies": {
    "chai": "=3.5.0",
    "coffee-coverage": "=1.0.1",
    "istanbul": "=0.4.4",
    "mocha": "=2.4.5",
    "mock-utf8-stream": "=0.1.1"
  },
  "bugs": {
    "url": "https://github.com/E-geek/ssi-cli/issues"
  },
  "homepage": "https://github.com/E-geek/ssi-cli#readme",
  "directories": {
    "test": "tests"
  }
}
